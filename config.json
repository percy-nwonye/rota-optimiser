    "export_folder": ".",
    "export_csv": true,
    "export_excel": true
  },
  "cover_suggestions": {
    "top_n": 3
  },
  "explainability": {
  "top_n_per_decision": 5
},
  "debug": {
    "print_no_candidate_ranking": false
  }
}
