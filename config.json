  "overrides": {
  "allow_overrides": true,
  "interactive_overrides": true,
  "warnings_enabled": true,

  "require_warning_ack": true,
  "warning_ack_phrase": "I UNDERSTAND",

},
  "scoring_weights": {
    "hours": 1.0,
    "nights": 15.0,
    "weekends": 10.0
  },
